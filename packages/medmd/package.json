{
  "name": "@minodisk/medmd",
  "version": "0.5.2",
  "description": "Converter markdown to HTML for Medium",
  "main": "lib/index.js",
  "repository": "git@github.com:minodisk/medic.git",
  "author": "Daisuke Mino <daisuke.mino@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build":
      "rm -rf lib/ && yarn flow-remove-types -d lib/ src/ && yarn format",
    "test": "yarn jest",
    "format": "yarn prettier --write *.json *.md src/*.js lib/*.js",
    "prepublish": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "transform": {
      "\\.js$": "../../bin/jest-transform"
    }
  },
  "dependencies": {
    "image-size": "^0.6.1",
    "mdurl": "^1.0.1",
    "rehype-parse": "^4.0.0",
    "rehype-remark": "^2.1.0",
    "remark-breaks": "^1.0.0",
    "remark-html": "^6.0.1",
    "remark-parse": "^4.0.0",
    "remark-stringify": "^4.0.0",
    "sync-request": "^4.1.0",
    "unified": "^6.1.6"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.2",
    "flow-bin": "^0.59.0",
    "flow-remove-types": "^1.2.3",
    "jest": "^21.2.1",
    "prettier": "^1.8.2"
  }
}
